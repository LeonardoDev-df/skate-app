{
  "name": "skate-app-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate-icons": "pwa-asset-generator public/skateboard.svg public/icons --background '#0f172a' --theme-color '#8b5cf6' --padding '10%' --scrape false --icon-only --favicon --mstile --apple-touch-icon",
    "generate-splash": "pwa-asset-generator public/skateboard.svg public/splash --background '#0f172a' --theme-color '#8b5cf6' --padding '20%' --scrape false --splash-only",
    "pwa-setup": "npm run generate-icons && npm run generate-splash"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "clsx": "^2.0.0",
    "firebase": "^10.7.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^1.14.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@jridgewell/source-map": "^0.3.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "pwa-asset-generator": "^6.4.0",
    "tailwindcss": "^3.3.6",
    "terser": "^5.24.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
